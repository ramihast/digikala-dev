    "postcss-cli": "^11.0.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "bootstrap": "^5.3.7",
    "jquery": "^3.7.1",
    "swiper": "^11.2.10"
  }
